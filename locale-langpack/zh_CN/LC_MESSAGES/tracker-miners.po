msgstr "要忽略的文件模式列表"

msgid "Log verbosity"
msgstr "日志详细度"

msgid "Log verbosity."
msgstr "日志的详细程度。"

msgid ""
"Logging, 0 = errors only, 1 = minimal, 2 = detailed and 3 = debug (default ="
" 0)"
msgstr "记录日志，0 = 只记录错误，1 = 最小， 2 = 详细， 3 = 调试 (默认 = 0)"

msgid ""
"Logging, 0 = errors only, 1 = minimal, 2 = detailed and 3 = debug "
"(default=0)"
msgstr "记录日志，0 = 只记录错误，1 = 最小，2 = 详细，3 = 调试 (默认 = 0)"

